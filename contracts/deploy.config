# Deployment Configuration for Base Sepolia
# Copy this to .env and fill in your values

# Your wallet private key (MUST include 0x prefix)
# Example: PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
PRIVATE_KEY=your_private_key_here

# Base Sepolia RPC URL (optional - will use default if not set)
RPC_URL=https://sepolia.base.org

# Etherscan API key for contract verification (optional)
# Get one at: https://basescan.org/apis
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas settings (optional - Foundry will auto-estimate)
GAS_LIMIT=3000000
GAS_PRICE=1000000000

